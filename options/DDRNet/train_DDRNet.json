{
    "task": "train_DDRNet"
    , "model": "DDRNet"
    , "gpu_ids": [0,1,2,3]
    , "num_gpu": 4
    , "dist": true
    , "find_unused_parameters": false
    , "use_static_graph": false
  
    ,"scale": 1
    , "n_channels": 3
  
    , "path": {
      "root": "experiments"
      , "pretrained_netG": null
      , "pretrained_netE": null
    }
  
    , "datasets": {
      "train": {
        "name": "train_dataset"
        , "dataset_type": "VideoRecurrentTrainDataset"
        , "dataroot_gt": "/dataset/VidUDC33K/"
        , "dataroot_lq": "/dataset/VidUDC33K/"
        , "filename_tmpl": "03d"
        , "filename_ext": "npy"
        , "test_mode": false
        , "io_backend": {"type": "npy"}
        , "num_frame": 30
        , "gt_size": 256
        , "interval_list": [1]
        , "random_reverse": false
        , "use_hflip": true
        , "use_rot": true
  
        , "dataloader_shuffle": true
        , "dataloader_num_workers": 20
        , "dataloader_batch_size": 8
      }
      , "test": {
        "name": "test_dataset"
        , "dataset_type": "VideoRecurrentTestDataset"
        , "dataroot_gt": "/dataset/VidUDC33K/"
        , "dataroot_lq": "/dataset/VidUDC33K/"
        , "cache_data": false
        , "io_backend": {"type": "npy"}
        , "num_frame": 50
        , "gt_size": [256,256]
      }
    }
  
    , "netG": {
      "net_type": "videoudc"
      , "mid_channels": 48
      , "scale_feat": 12
      , "num_blocks": 5
      , "tau": 0.9
      , "max_residue_magnitude": 10
      , "spynet_pretrained": "model_zoo/flow/spynet_sintel_final-3d2a1287.pth" // automatical download
      , "init_type": "default"
    }
  
  
    , "train": {
      "G_lossfn_type": "charbonnier"
      , "G_lossfn_weight": 1.0
      , "G_lossfn_weight_L1": 1.0
      , "G_lossfn_weight_L2": 1.0
      , "G_lossfn_weight_L3": 1.0
      , "G_charbonnier_eps": 1e-9
  
      , "E_decay": 0                      // Exponential Moving Average for netG: set 0 to disable; default setting 0.999
  
      , "G_optimizer_type": "adam"        // fixed, adam is enough
      , "G_optimizer_lr": 1e-4            // learning rate
      , "G_optimizer_betas": [0.9,0.99]
      , "G_optimizer_wd": 0               // weight decay, default 0
      , "G_optimizer_clipgrad": null      // unused
      , "G_optimizer_reuse": true         //
  
      , "fix_iter": 5000
       , "fix_lr_mul": 0.125
      , "fix_keys": ["spynet", "deform"]
  
      , "total_iter": 200000
      , "G_scheduler_type": "CosineAnnealingWarmRestarts"
      , "G_scheduler_periods": 200000
      , "G_scheduler_eta_min": 1e-7
  
      , "G_regularizer_orthstep": null    // unused
      , "G_regularizer_clipstep": null    // unused
  
      , "G_param_strict": true
      , "E_param_strict": true
  
      , "checkpoint_test": 5000           // for testing
      , "checkpoint_save": 5000           // for saving model
      , "checkpoint_print": 1000           // for print
    }
  
    , "val": {
      "save_img": true
      , "pad_seq": false
      , "flip_seq": false
      , "center_frame_only": false
      , "num_frame_testing": 12
      , "num_frame_overlapping": 2
      , "size_patch_testing": 256
    }
  
  }
  
